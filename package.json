  "dependencies": {
    "@ai-sdk/google": "^0.0.45",
    "@ai-sdk/openai": "^0.0.53",
    "@clack/prompts": "^0.7.0",
    "ai": "^3.3.17",
    "chalk": "^5.3.0",
